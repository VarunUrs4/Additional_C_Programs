//Program to find whether the entered numbered is a prime or a composite number.

#include<stdio.h>
int main()
{
    int i  ,num , flag=0;
    printf("\n Enter any number : ");
    scanf("%d",&num);
    for(i=2;i<=num/2;i++)
    {
      if(num%i==0)
     {
       flag=1;
       break;
     }
    }
    if(flag==1)
    printf("\n %d is a composite number\n",num);
    else
    printf("\n %d is a prime number\n",num);
    return 0;
}
