//Program to print the sum of squares of the even numbers.

#include<stdio.h>
int sq_even(int i, int n ,int s);
int main()
{
    int i=1 ,n,s=0;
    printf("\n Enter the value of n : ");
    scanf("%d",&n);
    sq_even(i,n,s);
    return 0;
}

int sq_even(int i, int n, int s)

{
    while(i<=n)
    {
    if(i%2!=0)
    printf("\n\t%d",i);
    if(i%2==0)
    s += (i*i);
    i++;
    }
    printf("\n\n Sum of square of even numbers : %d\n",s);
    return s;
}
