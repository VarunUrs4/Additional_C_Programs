//WAP to find factorial of a number
#include<stdio.h>
int factorial(int n);

int main()
{
    int n ,fact=1;
    printf("\n Enter the number : ");
    scanf("%d",&n);
    factorial(n);
    printf("\n Factorial of %d is : %d\n",n,factorial(n));
    return 0;
}
int factorial(int n)
{
   int i, fact=1;
   if(n==0)
   fact = 1;
   else
   {
     for(i=1;i<=n;i++)
      {
        fact = fact * i;
      }
    }
return fact;
}
